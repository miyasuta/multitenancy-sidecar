{
  "name": "multitenancy-sidecar-mtx",
  "dependencies": {
    "@sap/cds": "^6",
    "@sap/cds-mtxs": "^1",
    "@sap/xssec": "^3",
    "express": "^4",
    "hdb": "^0",
    "passport": "^0.6.0"
  },
  "devDependencies": {
    "sqlite3": "^5"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "cds.xt.ModelProviderService": "in-sidecar",
      "cds.xt.SaasProvisioningService": true,
      "cds.xt.DeploymentService": true,
      "cds.xt.ExtensibilityService": true,
      "[development]": {
        "db": {
          "kind": "sqlite",
          "credentials": {
            "url": "../../sqlite.db"
          }
        }
      }
    },
    "[development]": {
      "requires": {
        "auth": "dummy"
      },
      "server": {
        "port": 4005
      }
    }
  }
}